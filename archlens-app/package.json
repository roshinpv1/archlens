{
  "name": "CloudArc-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "init-checklist": "node scripts/initialize-checklist.js",
    "setup-embeddings": "./scripts/setup-embeddings.sh",
    "start-services": "echo 'Starting embeddings services...' && ollama serve & docker run -d --name qdrant-archlens -p 6333:6333 -p 6334:6334 qdrant/qdrant && echo 'Services started! Run npm run dev to start the app.'",
    "stop-services": "echo 'Stopping embeddings services...' && pkill -f ollama && docker stop qdrant-archlens && docker rm qdrant-archlens && echo 'Services stopped!'"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.7.0",
    "@types/file-saver": "^2.0.7",
    "@types/mongodb": "^4.0.6",
    "file-saver": "^2.0.5",
    "lucide-react": "^0.544.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
